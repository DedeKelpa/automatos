estados = [ "q1","q2"]
inicial = "q1"
aceitacao = ["q2"]
alfabeto = ["0","1"]
transicao = {
    "q1": { "0":"q1","1":"q2"},
    "q2": {"0":"q1","1":"q2"}
}
cadeia = input()
atual = inicial

for c in cadeia:
    if c not in alfabeto:
        atual = None
        break
    atual = transicao[atual][c]

if atual in aceitacao:
    print("Aceito")
else:
    print("Rejeito")

// Terminam em 1 ou tenham pelo menos 1 "1" e terminem em um n√∫mero par de 0 ( PARA O LAB )  
